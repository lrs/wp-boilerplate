- if ( have_posts() ) :

  :php
    while ( have_posts() ) : the_post();
      get_template_part( 'template-parts/content', 'page' );
      if ( comments_open() || get_comments_number() ) { comments_template(); }
    endwhile;

    the_posts_navigation();

- else :
  - get_template_part( 'template-parts/content', 'none' );

- endif
