{
	"extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "no-undef": 1,
    "no-unused-vars": 1,
    "indent": [1, 2, {"SwitchCase": 1}]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
  }
}
